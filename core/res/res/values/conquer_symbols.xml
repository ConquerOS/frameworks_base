<resources>

    <!-- Whether to cleanup fingerprints upon connection to the daemon and when user switches -->
    <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

    <!-- Seekbars -->
    <java-symbol type="dimen" name="seekbar_track_material_width" />
    <java-symbol type="dimen" name="seekbar_track_material_radius" />
    <java-symbol type="dimen" name="seekbar_track_material_height" />

    <!-- Switch Thumb -->
    <java-symbol type="dimen" name="switch_thumb_material_anim_width" />
    <java-symbol type="dimen" name="switch_thumb_material_anim_height" />
    <java-symbol type="dimen" name="switch_thumb_material_anim_stroke_width" />

    <!-- Switch Track Material -->
    <java-symbol type="dimen" name="switch_track_material_height" />
    <java-symbol type="dimen" name="switch_track_material_radius" />

    <!-- App error dialog -->
    <java-symbol type="id" name="aerr_copy" />
    <java-symbol type="string" name="url_copy_success" />
    <java-symbol type="string" name="url_copy_failed" />

    <!-- Whether to show a custom view for FOD -->
    <java-symbol type="bool" name="config_needCustomFODView" />

    <!-- Whether device supports internal audio recording -->
    <java-symbol type="bool" name="config_hasInternalAudioRecordingSupport" />

    <!-- Device have OnePlus Vibration mototr -->
    <java-symbol type="bool" name="config_hasOnePlusHapticMotor" />

    <!-- Whether to show WiFi/Hotspot generation number -->
    <java-symbol type="bool" name="config_show_network_generation" />

    <java-symbol type="string" name="reboot_title" />
    <java-symbol type="string" name="reboot_confirm" />

    <!-- Power menu reboot -->
    <java-symbol name="reboot_to_bootloader_title" type="string" />
    <java-symbol name="reboot_to_bootloader_message" type="string" />
    <java-symbol name="reboot_to_recovery_title" type="string" />
    <java-symbol name="reboot_to_recovery_message" type="string" />
    <java-symbol name="reboot_system_title" type="string" />
    <java-symbol name="reboot_system_message" type="string" />
    <java-symbol type="array" name="config_rebootActionsList" />

    <!-- Button backlight -->
    <java-symbol type="integer" name="config_buttonBrightnessSettingDefault" />
    <java-symbol type="bool" name="config_deviceHasVariableButtonBrightness" />

    <!-- Whether device supports variable refresh rate -->
    <java-symbol type="bool" name="config_hasVariableRefreshRate" />

</resources>
